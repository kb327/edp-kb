<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edp.wat.edu.pl.projectkb.controller.RegisterViewController">
   <children>
      <VBox layoutX="234.0" layoutY="28.0" prefHeight="394.0" prefWidth="430.0">
         <children>
            <Label text="Podaj nazwę użytkownika" />
            <TextField fx:id="usernameText" promptText="username" />
            <TextField fx:id="emailText" promptText="email" />
            <PasswordField fx:id="passwdText" promptText="password" />
            <PasswordField fx:id="passwdSecText" promptText="password" />
            <TextField fx:id="nameText" promptText="name" />
            <TextField fx:id="surnameText" promptText="surname" />
            <Button mnemonicParsing="false" onMouseClicked="#register" text="Zarejestruj" />
         </children>
      </VBox>
   </children>
</AnchorPane>
